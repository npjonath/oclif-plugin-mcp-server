{
  "name": "oclif-plugin-mcp-server",
  "description": "A plugin for the oclif CLI framework that automatically discovers commands and serves them through the Model Context Protocol (MCP).",
  "version": "0.2.0",
  "author": "Jonathan Jot",
  "bin": {
    "oclif-plugin-mcp-server": "./bin/run.js"
  },
  "bugs": "https://github.com/npjonath/oclif-plugin-mcp-server/issues",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.2",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19",
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/sinon": "^17.0.4",
    "chai": "^4",
    "commitlint": "^19",
    "husky": "^9.1.7",
    "lint-staged": "^15",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "nyc": "^15.1.0",
    "oclif": "^4",
    "prettier": "^3.5.3",
    "shx": "^0.4.0",
    "ts-node": "^10",
    "sinon": "^21.0.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/npjonath/oclif-plugin-mcp-server",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "oclif-plugin-mcp-server",
    "dirname": "oclif-plugin-mcp-server",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "npjonath/oclif-plugin-mcp-server",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "clean": "shx rm -f oclif.manifest.json",
    "compile": "tsc",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "packageManager": "yarn@1.22.19+sha512.ff4579ab459bb25aa7c0ff75b62acebe576f6084b36aa842971cf250a5d8c6cd3bc9420b22ce63c7f93a0857bc6ef29291db39c3e7a23aab5adfd5a4dd6c5d71"
}
