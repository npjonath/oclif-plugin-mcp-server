{
  "name": "oclif-plugin-mcp-server",
  "description": "A plugin for the oclif CLI framework that automatically discovers commands and serves them through the Model Context Protocol (MCP).",
  "version": "0.6.0",
  "author": "Jonathan Jot",
  "bugs": "https://github.com/npjonath/oclif-plugin-mcp-server/issues",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.0",
    "@oclif/core": "^4",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19",
    "@eslint/compat": "^1",
    "@oclif/plugin-help": "^6",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/mocha": "^10",
    "@types/node": "^24",
    "@types/sinon": "^17.0.4",
    "@types/uuid": "^10.0.0",
    "chai": "^5",
    "commitlint": "^19",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "husky": "^9.1.7",
    "lint-staged": "^16",
    "mocha": "^11",
    "nyc": "^17.1.0",
    "oclif": "^4",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "sinon": "^21.0.0",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/npjonath/oclif-plugin-mcp-server",
  "keywords": [
    "oclif-plugin"
  ],
  "exports": "./dist/index.js",
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "oclif-plugin-mcp-server",
    "dirname": "oclif-plugin-mcp-server",
    "commands": "./dist/commands",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "flexibleTaxonomy": true,
    "topicSeparator": " "
  },
  "repository": "npjonath/oclif-plugin-mcp-server",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "clean": "shx rm -f oclif.manifest.json",
    "compile": "tsc",
    "lint": "eslint",
    "postpack": "yarn run clean",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "husky || true && yarn build && oclif manifest",
    "pretest": "yarn build && tsc -p test --noEmit",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{js,ts,json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "types": "dist/index.d.ts",
  "packageManager": "yarn@1.22.22"
}
